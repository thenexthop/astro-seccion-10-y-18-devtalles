generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model client {
  id        String  @id(map: "client_pk") @db.Uuid
  name      String  @db.VarChar
  age       Int?    @default(0)
  is_active Boolean @default(true)
}
